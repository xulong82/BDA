}
model {
beta ~ multi_normal(mu, Sigma);
}
"
stan_multi_cholesky <- "
data {
int<lower=1> K; # outcomes
vector[K] mu;
cov_matrix[K] Sigma;
}
transformed data {
matrix[K, K] L;
L <- cholesky_decompose(Sigma);
}
parameters {
vector[K] beta;
}
model {
beta ~ multi_normal_cholesky(mu, L);
}
"
stan_cholesky <- "
data {
int<lower=1> K; # outcomes
vector[K] mu;
cov_matrix[K] Sigma;
}
transformed data {
matrix[K, K] L;
L <- cholesky_decompose(Sigma);
}
parameters {
vector[K] alpha;
}
transformed parameters {
vector[K] beta;
beta <- mu + L * alpha;
}
model {
alpha ~ normal(0, 1);
}
"
